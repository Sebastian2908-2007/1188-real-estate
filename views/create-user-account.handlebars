<form id="user-signup-form" class="form">
    <div class="signup-title-div">
        <h1 id="signup-form-title">Fill Out The Form To Create Your Account</h1>
    </div>
  <div class="label-input-container" > 
    <label for="username-signup">Username</label>
    <input type="text" id="username-signup" />
 </div>

  <div class="label-input-container"> 
    <label for="state">State</label>
    <input type="text" id="state">
  </div> 

   <div class="label-input-container" >
    <label for="email-signup">Email</label>
    <input type="text" id="email-signup">
   </div>

  <div class="label-input-container"> 
    <label for="password-signup">Password</label>
    <input type="password" id="password-signup">
  </div>

<div class="signup-btn-container">
    <button class="signup-btn"type="submit">Create Account</button>
</div>

</form>
<script src="/javascript/create-user.js"></script>
<script src="/javascript/user-logout.js"></script>